<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eliorcohen12345.locationproject"
    android:versionCode="89"
    android:versionName="1.0.0">

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.ConApp"
        android:allowBackup="true"
        android:icon="@drawable/mapsicon"
        android:largeHeap="true"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mapsicon"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/*" />
            </intent-filter>
        </activity>
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.ActivityFavorites" />
        <activity android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.Credits" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.DeleteAllDataFavorites" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.AddPlaceFavorites" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.EditPlace" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.DeletePlace" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.AddMarkerFavorites" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.PlaceCustomActivity" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.DeleteAllDataHistory" />
        <activity android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.SettingsActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/api_key_map" />

        <service android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>